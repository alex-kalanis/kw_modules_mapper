{
    "name": "alex-kalanis/kw_modules_mapper",
    "description": "Selecting correct modules - use mapper from KWCMS",
    "keywords": [
        "module",
        "modules"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_mapper": ">=3.0 <4",
        "alex-kalanis/kw_modules": ">=2.0 <3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "php": ">=7.4.0"
    },
    "require-dev": {
        "alex-kalanis/kw_storage": ">=5.0 <6",
        "shipmonk/composer-dependency-analyser": "^1.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=8.0 <=9"
    },
    "suggest": {
        "alex-kalanis/kw_files": "Accessing stored entries as files on preset volume",
        "alex-kalanis/kw_storage": "Key-value storage for accessing data"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_modules\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
